<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qf.dao.GoodsTypeMapper">

    <select id="findTypeByLevel" resultType="com.qf.pojo.GoodsType">
        select * from tb_goods_type where level = #{level}
    </select>
    <select id="findById" resultType="com.qf.pojo.GoodsType">
        select * from tb_goods_type where id = #{id}
    </select>
</mapper>